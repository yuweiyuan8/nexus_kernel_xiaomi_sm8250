# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# QCACLD
CONFIG_QCA_CLD_WLAN=y
CONFIG_QCA_CLD_WLAN_PROFILE="qca6390"

# DebugFS
CONFIG_DEBUG_FS=n
CONFIG_MSM_TZ_LOG=n
CONFIG_CMA_DEBUGFS=n
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=n
CONFIG_IPA_UT=n
CONFIG_IOMMU_TESTS=n
CONFIG_IOMMU_TESTS=n
CONFIG_QMP_DEBUGFS_CLIENT=n

# exFAT
CONFIG_EXFAT_FS=y

# erofs
CONFIG_EROFS_FS=y

# Persistent store
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
CONFIG_CMDLINE="cgroup_disable=pressure ramoops_memreserve=4M"
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_OOPS=y
CONFIG_MTD_BLOCK2MTD=y

# Backlight
CONFIG_BACKLIGHT_KTZ8866=y
CONFIG_BACKLIGHT_DUALKTZ8866=y

# Camera motor
CONFIG_MOTOR_DRV8846=y

# GPS
CONFIG_AIROHA_GPS=y

# Hall sensor
CONFIG_HALL_AKM09970=y

# ION
CONFIG_ION_CAMERA_HEAP=y

# IR
CONFIG_RC_CORE=y
CONFIG_LIRC=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI_MI=y

# Keyboard
CONFIG_INPUT_XIAOMI_KEYBOARD=y
CONFIG_KEYBOARD_NANO_8030=y

# LED
CONFIG_LEDS_AW2015=y

# Power
CONFIG_FRAME_WARN=4096
CONFIG_SMB1355_SLAVE_CHARGER=n
CONFIG_SMB1390_CHARGE_PUMP_PSY=n
CONFIG_SMB1398_CHARGER=n

# Regulator
CONFIG_I2C_RETRY=y
CONFIG_REGULATOR_PM8008=y
CONFIG_REGULATOR_QCOM_RPMH=y

# Touchscreen
CONFIG_TOUCHSCREEN_FTS=n
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX=n
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM=n
CONFIG_TOUCHSCREEN_ST=n
CONFIG_TOUCHSCREEN_COMMON=y

# USB
CONFIG_PS5169=y

# Vibrator
CONFIG_INPUT_QTI_HAPTICS=n

# Process reclaim
CONFIG_PROCESS_RECLAIM=n

# Thermal
CONFIG_QTI_CPU_ISOLATE_COOLING_DEVICE=n

# SLMK
CONFIG_PSI=n
CONFIG_MEMCG=n
CONFIG_MEMCG_SWAP=n
CONFIG_ANDROID_SIMPLE_LMK=y

# In-Kernel boost
CONFIG_MSM_PERFORMANCE=n
CONFIG_CPU_BOOST=n

# Debloat
CONFIG_AUDIT=n
CONFIG_SECURITY_APPARMOR=n
CONFIG_IKHEADERS=n
CONFIG_EDAC=n
CONFIG_CPU_FREQ_TIMES=n
CONFIG_HARDEN_EL2_VECTORS=n
CONFIG_SLUB_CPU_PARTIAL=n
CONFIG_ARM64_SW_TTBR0_PAN=n
CONFIG_VMAP_STACK=n
CONFIG_NL80211_TESTMODE=n
CONFIG_ALLOW_DEV_COREDUMP=n
CONFIG_PPP=n
CONFIG_MMC=n
CONFIG_VIRTIO_MENU=n
CONFIG_IPA3_REGDUMP=n
CONFIG_COMMON_CLK_XGENE=n
CONFIG_MSM_DEBUGCC_KONA=n
CONFIG_QTI_RPM_STATS_LOG=n
CONFIG_QTI_DDR_STATS_LOG=n
CONFIG_PWM_QTI_LPG=n
CONFIG_RAS=n
CONFIG_DEBUG_INFO=n
CONFIG_MAGIC_SYSRQ_SERIAL=n
CONFIG_SCHEDSTATS=n
CONFIG_IPC_LOGGING=n
CONFIG_DEBUG_LIST=n
CONFIG_DEBUG_ALIGN_RODATA=n
CONFIG_CORESIGHT=n
CONFIG_RUNTIME_TESTING_MENU=n
CONFIG_SECURITY_SMACK=n

# CRYPTO
CONFIG_CRYPTO_LZ4=y
CONFIG_CRYPTO_LZO=n
CONFIG_ZRAM_DEFAULT_COMP_ALGORITHM="lz4"

# Module
CONFIG_MODULES=n

# Suspend skip sync
CONFIG_SUSPEND_SKIP_SYNC=y

# Devfreq boost
CONFIG_DEVFREQ_BOOST=y
CONFIG_DEVFREQ_INPUT_BOOST_DURATION_MS=58
CONFIG_DEVFREQ_WAKE_BOOST_DURATION_MS=1000
CONFIG_DEVFREQ_CPU_LLCC_DDR_BW_BOOST_FREQ=5931

# Coresight
CONFIG_CORESIGHT_PLACEHOLDER=y
CONFIG_CORESIGHT_AMBA_PLACEHOLDER=y